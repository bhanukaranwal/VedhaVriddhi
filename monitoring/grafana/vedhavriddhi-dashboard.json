{
  "dashboard": {
    "id": null,
    "title": "VedhaVriddhi Universal Financial Intelligence System",
    "tags": ["vedhavriddhi", "phase4", "quantum", "agi"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"vedhavriddhi-services\"}",
            "legendFormat": "Services Up"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 8},
                {"color": "green", "value": 12}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Quantum Processing Status",
        "type": "graph",
        "targets": [
          {
            "expr": "quantum_jobs_total",
            "legendFormat": "Total Quantum Jobs"
          },
          {
            "expr": "quantum_jobs_completed",
            "legendFormat": "Completed Jobs"
          }
        ]
      },
      {
        "id": 3,
        "title": "AGI Agent Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "agi_agents_active",
            "legendFormat": "Active AGI Agents"
          },
          {
            "expr": "agi_reasoning_requests_per_sec",
            "legendFormat": "Reasoning Requests/sec"
          }
        ]
      },
      {
        "id": 4,
        "title": "Consciousness Level",
        "type": "gauge",
        "targets": [
          {
            "expr": "global_consciousness_level",
            "legendFormat": "Global Consciousness"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.5},
                {"color": "green", "value": 0.8}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Planetary Health Score",
        "type": "gauge",
        "targets": [
          {
            "expr": "planetary_health_score",
            "legendFormat": "Planetary Health"
          }
        ]
      },
      {
        "id": 6,
        "title": "DeFi Protocol Status",
        "type": "table",
        "targets": [
          {
            "expr": "defi_protocols_connected",
            "legendFormat": "Connected Protocols"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
